    <!-- Innovation Tools Section -->
    <section class="innovation-section">
        <div class="container">
            <div class="innovation-grid">
                <div class="innovation-item">
                    <div class="innovation-image">
                        <img src="assets/media/umbrella-logo.png" alt="AI Glasses Innovation">
                    </div>
                    <div class="innovation-content">
                        <h2>Access tools built for innovation</h2>
                        <p>Enjoy early access to our SDK, testing tools and documentation to help you kick start building with AI glasses.</p>
                    </div>
                </div>
                <div class="innovation-item reverse">
                    <div class="innovation-image">
                        <img src="assets/media/umbrellaproject.png" alt="Umbrella Project Device">
                    </div>
                    <div class="innovation-content">
                        <h2>Environmental Intelligence Platform</h2>
                        <p>A compact 10 cm module that converts environmental data into over 100 unique parameters, generating millions of real data points for research and autonomous systems.</p>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Features Section -->

    <!-- Technical Specs Section -->
    <section class="specs-section">
        <div class="container">
            <h2 class="section-title">Technical Specifications</h2>
            <div class="specs-grid">
                <div class="spec-category">
                    <h3>Hardware</h3>
                    <ul>
                        <li>Temperature/Humidity: DHT22 or BME280</li>
                        <li>Pressure Sensor: BME280</li>
                        <li>Virtual Radiation: Solar panel + TEG</li>
                        <li>Microcontroller: Low-power embedded</li>
                        <li>Power: Solar with battery backup</li>
                    </ul>
                </div>
                <div class="spec-category">
                    <h3>Accuracy</h3>
                    <ul>
                        <li>Temperature: ±0.5°C</li>
                        <li>Humidity: ±2-3% RH</li>
                        <li>Pressure: ±1 hPa</li>
                        <li>Radiation: ±15%</li>
                        <li>ET Prediction: ±10-15%</li>
                    </ul>
                </div>
                <div class="spec-category">
                    <h3>Performance</h3>
                    <ul>
                        <li>100+ environmental parameters</li>
                        <li>Milliliter-level irrigation precision</li>
                        <li>Multi-zone management</li>
                        <li>Machine learning integration</li>
                        <li>Adaptive algorithms</li>
                    </ul>
                </div>
            </div>
        </div>
    </section>



